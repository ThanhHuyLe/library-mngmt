<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<GridPane prefHeight="284.0" prefWidth="359.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="business.BookController">
  <children>
    <TableView fx:id="authorTable" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="0">
      <columns>
        <TableColumn prefWidth="75.0" text="author ID" fx:id="autherIdCol" />
        <TableColumn prefWidth="75.0" text="Firstname" fx:id="fnameCol" />
        <TableColumn prefWidth="75.0" text="Lastname" fx:id="lnameCol" />
        <TableColumn prefWidth="75.0" text="telephone" fx:id="telephoneCol" />
      </columns>
    </TableView>
    <HBox alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="1">
      <children>
        <Button mnemonicParsing="false" onAction="#selectAuthor" text="Button" />
      </children>
    </HBox>
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
</GridPane>
