<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<GridPane prefHeight="450.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
   </rowConstraints>
   <children>
      <Label text="Check Book Available">
         <padding>
            <Insets left="20.0" top="20.0" />
         </padding>
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <GridPane prefHeight="93.0" prefWidth="700.0" GridPane.rowIndex="2">
        <columnConstraints>
          <ColumnConstraints />
          <ColumnConstraints />
            <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="306.0" minWidth="10.0" prefWidth="242.0" />
            <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="306.0" minWidth="10.0" prefWidth="242.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <padding>
            <Insets left="60.0" right="40.0" top="20.0" />
         </padding>
         <children>
            <Label text="Member ID" GridPane.columnIndex="2" GridPane.halignment="LEFT" GridPane.valignment="CENTER">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="memberID" GridPane.columnIndex="3" />
            <Label text="ISBN" GridPane.columnIndex="2" GridPane.rowIndex="1">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="isbn" GridPane.columnIndex="3" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="reset" mnemonicParsing="false" text="Reset" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets left="610.0" />
         </GridPane.margin>
      </Button>
      <Button fx:id="checkBtn" mnemonicParsing="false" text="Check" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets left="520.0" />
         </GridPane.margin>
      </Button>
      <Label text="Book Status" GridPane.rowIndex="5">
         <font>
            <Font size="18.0" />
         </font>
         <padding>
            <Insets left="20.0" />
         </padding>
      </Label>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="6">
         <children>
            <TableView prefHeight="30.0" prefWidth="600.0">
              <columns>
                <TableColumn fx:id="memberCol" prefWidth="135.0" text="Member ID" />
                <TableColumn fx:id="isbnCol" prefWidth="149.0" text="ISBN" />
                  <TableColumn fx:id="checkoutDateCol" prefWidth="161.0" text="Checkout Date" />
                  <TableColumn fx:id="dueDateCol" prefWidth="154.0" text="Due Date" />
              </columns>
            </TableView>
         </children>
      </HBox>
      <Button fx:id="backBtn" mnemonicParsing="false" text="&lt;= Back to Main" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets left="20.0" />
         </GridPane.margin>
      </Button>
   </children>
</GridPane>
